{
  "name": "ts-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "console": "DEBUG=express-session ts-node-dev -r tsconfig-paths/register src/nest-repl.ts",
    "db": "rimraf node_modules/.prisma && prisma generate",
    "db-migrate": "prisma migrate dev",
    "db-prod": "prisma migrate deploy",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "dev-hmr": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "dev-debug": "nest start --debug --watch",
    "start": "nest start",
    "start-prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "up-orm": "shx pnpm add -D prisma@latest; shx pnpm add @prisma/client@latest"
  },
  "resolutions": {
    "tough-cookie": "4.0.0",
    "@types/express-session": "Seiyial/types-express-session-custom"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.54.0",
    "@aws-sdk/credential-providers": "^3.54.0",
    "@aws-sdk/signature-v4-crt": "^3.54.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "3.11.0",
    "@quixo3/prisma-session-store": "3.1.2",
    "argon2": "^0.28.4",
    "cookie-signature": "^1.1.0",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express-session": "^1.17.2",
    "purdy": "^3.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@syxdaybreak/eslint-config": "^1.0.5",
    "@types/express": "^4.17.13",
    "@types/express-session": "Seiyial/types-express-session-custom",
    "@types/jest": "^27.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.22",
    "@types/supertest": "^2.0.11",
    "@types/tough-cookie": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chalk": "4.1.2",
    "eslint": "8.2.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-no-eslint-disable": "1.0.1",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "got": "^11.8.2",
    "jest": "^27.2.5",
    "morgan": "^1.10.0",
    "prettier": "^2.3.2",
    "prisma": "^3.11.0",
    "rimraf": "^3.0.2",
    "run-script-webpack-plugin": "^0.0.11",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "tough-cookie": "^4.0.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
